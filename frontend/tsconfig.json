{
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "allowJs": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "types": ["node", "vite/client", "vite-plugin-svgr/client"],
  },
  "include": ["./src/**/*", "./typings", "vite.config.ts"]
}
