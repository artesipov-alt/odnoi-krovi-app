root = true

[*]
end_of_line = crlf
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

[*.{jsx,js,ts,tsx,html}]
indent_style = space
indent_size = 4

[*.{css,less}]
indent_style = space
indent_size = 4

[package.json]
indent_style = space
indent_size = 2
