{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx src --report-unused-disable-directives --max-warnings 0",
    "lint:ts:fix": "eslint . --ext .js,.jsx,.ts,.tsx src --fix",
    "lint:pre-commit": "eslint --config ../eslint.precommit.json --ext .js,.jsx,.ts,.tsx src --max-warnings=0 --cache",
    "prettier:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit -p .",
    "pre-commit": "npm run typecheck && npm run lint:pre-commit && npm run test:pre-commit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^6.0.0",
    "@pbe/react-yandex-maps": "^1.2.5",
    "@twa-dev/sdk": "^8.0.2",
    "axios": "^1.12.2",
    "dayjs": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "react-toastify": "^11.0.5",
    "vite": "^5.0.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^5.0.4",
    "axios-mock-adapter": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-diff": "^2.0.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
